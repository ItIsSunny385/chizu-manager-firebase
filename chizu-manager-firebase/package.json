{
    "name": "chizu-manager-firebase",
    "version": "0.0.2",
    "description": "Chizu Manager Application by Firebase",
    "main": "firebaseFunctions.js",
    "scripts": {
        "dev": "next src/",
        "build": "next build src/",
        "start": "next start src/",
        "serve": "npm run build && firebase emulators:start --only functions,hosting,auth,firestore --import=data --export-on-exit",
        "shell": "npm run build && firebase functions:shell",
        "deploy": "firebase deploy --only functions,hosting,auth,firestore",
        "logs": "firebase functions:log"
    },
    "dependencies": {
        "@react-google-maps/api": "^2.1.1",
        "basic-auth-connect": "^1.0.0",
        "bootstrap": "^4.6.0",
        "express": "^4.17.1",
        "firebase-admin": "^9.4.2",
        "firebase-functions": "^3.13.1",
        "next": "latest",
        "next-routes": "^1.4.2",
        "nookies": "^2.5.2",
        "react": "^17.0.1",
        "react-bootstrap-icons": "^1.4.0",
        "react-bootstrap-table-next": "^4.0.3",
        "react-bootstrap-table2-paginator": "^2.1.2",
        "react-dom": "^17.0.1",
        "reactstrap": "^8.9.0"
    },
    "devDependencies": {
        "@types/firebase": "^3.2.1",
        "@types/googlemaps": "^3.43.3",
        "@types/node": "^14.14.37",
        "@types/react": "^17.0.3",
        "@types/react-bootstrap-table-next": "^4.0.13",
        "@types/reactstrap": "^8.7.2",
        "firebase-functions-test": "^0.2.3",
        "firebase-tools": "^9.3.0",
        "typescript": "^4.2.3"
    },
    "license": "MIT"
}
